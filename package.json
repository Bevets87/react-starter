{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "A basic fullstack react starter setup",
  "main": "./src/server/app.prod.js",
  "scripts": {
    "build": "webpack --config ./webpack.config.prod.js -p --progress --colors",
    "remove-build": "node_modules/.bin/rimraf ./dist",
    "dev": "nodemon --watch src/server --exec babel-node -- ./src/server/app.dev.js",
    "prod": "babel-node ./src/server/app.prod.js",
    "start": "babel-node ./src/server/app.prod.js"
  },
  "author": "steve barton",
  "license": "ISC",
