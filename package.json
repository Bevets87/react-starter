    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.17.1",
    "bootstrap": "^3.3.7",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "lodash": "^4.17.4",
    "mongoose": "^4.9.6",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1"
  },
  "devDependencies": {
    "babel-loader": "^7.0.0",
